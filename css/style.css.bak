@import url('https://fonts.googleapis.com/css2?family=Poppins:wght@100;200;300;400;500;600;700;800;900&display=swap');

*{
   margin:0;
   padding:0;
   box-sizing:border-box;
   outline:none;
}
body{
	font-family: 'Poppins', sans-serif;
}
:root{
	--primary:#dc3545;
	--secondary:#c61227;
	--light:#4c4d4d;
	--dark:#252b33;
	--textlight:#4c4d4d;
	--space15:15px;
	--white:white;
	--pink:#fcebed;
}
.container{
	max-width:1140px;
	margin:auto;
}
.row{
	display:flex;
	flex-wrap:wrap;
}
ul,li{
	list-style:none;
}
a{
	text-decoration:none;
}
p{
	line-height:1.9;
}
.color{
	background-color:var(--pink);
}
img{
	max-width:100%;
}
.top i{
	width: 40px;
	height: 40px;
	font-size: 14px;
	display: block;
	text-align: center;
	line-height: 40px;
	border-radius: 4px 0px 0px 4px;
	background-color: rgba(0,0,0,0.2);
	text-decoration: none;
	position: fixed;
	right: 0px;
	bottom: 380px;
	box-shadow :0px 0px 3px 1px rgb(0,0,0.1);
	color: var(--white);
	font-size: 21px;
	background-color: rgb(51, 51, 51);
}

.backtotop i{
	width: 36px;
	height: 36px;
	font-size: 14px;
	display: block;
	text-align: center;
	line-height: 36px;
	border-radius: 50%;
	background-color: rgba(0,0,0,0.2);
	text-decoration: none;
	position: fixed;
	right: 10px;
	bottom: 10px;
    box-shadow: 0px 5px 15px rgb(0 0 0 / 15%);
	transition: all 0.5s;
	color: var(--white);
	font-size: 16px;
	background-color: rgba(0, 0, 0, 0.2)
}

.backtotop i:hover{
		background-color:var(--primary);
}
.pad75{
	padding: 75px var(--space15);
}
